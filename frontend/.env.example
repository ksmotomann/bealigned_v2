# Example environment variables for BeAligned Frontend
# Copy this file to .env.local for local development
# Copy this file to .env.production for production deployment

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# API Configuration
REACT_APP_API_URL=your_api_url

# For local development:
# REACT_APP_SUPABASE_URL=http://localhost:54321
# REACT_APP_SUPABASE_ANON_KEY=<get-from-supabase-start-output>
# REACT_APP_API_URL=http://localhost:3001

# For production:
# REACT_APP_SUPABASE_URL=https://qujysevuyhqyitxqctxg.supabase.co
# REACT_APP_SUPABASE_ANON_KEY=<your-production-anon-key>
# REACT_APP_API_URL=https://qujysevuyhqyitxqctxg.supabase.co/functions/v1