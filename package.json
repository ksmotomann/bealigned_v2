    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "expo": "54.0.3",
    "expo-clipboard": "^8.0.7",
    "expo-constants": "~18.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.1",
    "expo-status-bar": "~3.0.8",
    "lucide-react-native": "^0.544.0",
