{
  "name": "bealigned_v2",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:fresh": "./clear-cache.sh",
    "start:clear": "expo start --clear",
    "clear-cache": "./clear-cache.sh",
    "reset": "rm -rf node_modules/.cache .expo && npm start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && cp figma-wrapper.html dist/",
    "build:preview": "eas build --platform all --profile preview",
    "build:production": "eas build --platform all --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update --auto"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
