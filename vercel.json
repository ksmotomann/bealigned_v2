{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/grounding/:week",
      "destination": "/api/og/grounding"
    },
    {
      "source": "/(login|signup|dashboard|chat|profile|settings|admin|history|subscription|welcome|session|faq|our-story)/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(login|signup|dashboard|chat|profile|settings|admin|history|subscription|welcome|session|faq|our-story)",
      "destination": "/index.html"
    },
    {
      "source": "/_expo/:path*",
      "destination": "/_expo/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/figma-wrapper.html"
    }
  ]
}