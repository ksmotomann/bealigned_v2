{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/grounding/:week",
      "destination": "/api/og/grounding"
    }
  ],
  "routes": [
    {
      "src": "^/(about|pricing|contact|features)?$",
      "dest": "/figma-wrapper.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}