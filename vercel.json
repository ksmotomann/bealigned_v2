{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/grounding/:week",
      "destination": "/api/og/grounding"
    },
    {
      "source": "/(about|pricing|contact|features)",
      "destination": "/figma-wrapper.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}