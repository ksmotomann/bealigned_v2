{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/grounding/:week",
      "destination": "/api/og/grounding"
    },
    {
      "source": "/about",
      "destination": "/figma-wrapper.html"
    },
    {
      "source": "/pricing",
      "destination": "/figma-wrapper.html"
    },
    {
      "source": "/contact",
      "destination": "/figma-wrapper.html"
    },
    {
      "source": "/features",
      "destination": "/figma-wrapper.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}