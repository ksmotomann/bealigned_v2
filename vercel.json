{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "^/(about|pricing|contact|features)?$",
      "dest": "/figma-wrapper.html"
    },
    {
      "src": "^/grounding/([0-9]+)$",
      "dest": "/api/og/grounding?week=$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}